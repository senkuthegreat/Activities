  "version": "1.0.1",
  "logo": "https://cdn.rcd.gg/PreMiD/websites/M/MineHoster/assets/logo.png",
  "thumbnail": "https://cdn.rcd.gg/PreMiD/websites/M/MineHoster/assets/thumbnail.png",
  "color": "#3B82F6",
  "category": "other",
  "tags": [
    "minecraft",
    "hosting",
    "server",
    "german",
    "gameserver"
  ],
  "settings": [
    {
      "id": "lang",
      "multiLanguage": true
    },
    {
      "id": "showGamepanelName",
      "title": "Show Gamepanel in Activity Name",
      "icon": "fas fa-terminal",
      "value": true
    },
    {
      "id": "showButtons",
      "title": "Show Buttons",
      "icon": "fas fa-compress-arrows-alt",
      "value": true
    },
    {
      "id": "showSearchActivity",
      "title": "Show Search Activity",
      "icon": "fas fa-search",
      "value": true
